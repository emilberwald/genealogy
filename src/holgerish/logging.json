{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "explicit": {
      "format": "[%(levelname)s][%(name)s][%(asctime)s][%(relativeCreated)07dms][%(processName)s:%(threadName)s][%(pathname)s:%(lineno)s][%(funcName)s]\n%(message)s\n"
    }
  },
  "filters": {
    "filterCRITICAL": {
      "()": "holgerish.configure.LevelFilter",
      "levelname": "CRITICAL"
    },
    "filterERROR": {
      "()": "holgerish.configure.LevelFilter",
      "levelname": "ERROR"
    },
    "filterWARNING": {
      "()": "holgerish.configure.LevelFilter",
      "levelname": "WARNING"
    },
    "filterINFO": {
      "()": "holgerish.configure.LevelFilter",
      "levelname": "INFO"
    },
    "filterDEBUG": {
      "()": "holgerish.configure.LevelFilter",
      "levelname": "DEBUG"
    },
    "filterNOTSET": {
      "()": "holgerish.configure.LevelFilter",
      "levelname": "NOTSET"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "explicit",
      "stream": "ext://sys.stdout"
    },
    "filehandlerCRITICAL": {
      "class": "logging.handlers.RotatingFileHandler",
      "filters": [
        "filterCRITICAL"
      ],
      "level": "CRITICAL",
      "formatter": "explicit",
      "filename": "level-CRITICAL.log",
      "maxBytes": 10485760,
      "backupCount": 2
    },
    "filehandlerERROR": {
      "class": "logging.handlers.RotatingFileHandler",
      "filters": [
        "filterERROR"
      ],
      "level": "ERROR",
      "formatter": "explicit",
      "filename": "level-ERROR.log",
      "maxBytes": 10485760,
      "backupCount": 2
    },
    "filehandlerWARNING": {
      "class": "logging.handlers.RotatingFileHandler",
      "filters": [
        "filterWARNING"
      ],
      "level": "WARNING",
      "formatter": "explicit",
      "filename": "level-WARNING.log",
      "maxBytes": 10485760,
      "backupCount": 2
    },
    "filehandlerINFO": {
      "class": "logging.handlers.RotatingFileHandler",
      "filters": [
        "filterINFO"
      ],
      "level": "INFO",
      "formatter": "explicit",
      "filename": "level-INFO.log",
      "maxBytes": 10485760,
      "backupCount": 2
    },
    "filehandlerDEBUG": {
      "class": "logging.handlers.RotatingFileHandler",
      "filters": [
        "filterDEBUG"
      ],
      "level": "DEBUG",
      "formatter": "explicit",
      "filename": "level-DEBUG.log",
      "maxBytes": 10485760,
      "backupCount": 2
    },
    "filehandlerNOTSET": {
      "class": "logging.handlers.RotatingFileHandler",
      "filters": [
        "filterNOTSET"
      ],
      "level": "NOTSET",
      "formatter": "explicit",
      "filename": "level-NOTSET.log",
      "maxBytes": 10485760,
      "backupCount": 2
    }
  },
  "root": {
    "level": "NOTSET",
    "handlers": [
      "console",
      "filehandlerCRITICAL",
      "filehandlerERROR",
      "filehandlerWARNING",
      "filehandlerINFO",
      "filehandlerDEBUG",
      "filehandlerNOTSET"
    ],
    "propagate": false
  },
  "loggers": {
    "__main__": "cfg://root",
    "hoglerish.html_parser": "cfg://root"
  }
}